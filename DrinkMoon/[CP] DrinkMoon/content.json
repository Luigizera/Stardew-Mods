{
   //ATENÇÃO: SPOILERS ABAIXO.
   "Format": "2.0.0",
   
   "ConfigSchema": 
   {
      "SeasonalOutfits": 
      {
         "Default": "true",
         "AllowValues": "true, false",
      },
      "HalloweenOutfits": 
      {
         "Default": "true",
         "AllowValues": "true, false",
      }
   },

   "Changes":
   [
      {
         //Adiciona os Mapas
         "Action": "Include",
         "FromFile": "Data/NewMaps/NewMaps.json"
      },

      {
         //Adiciona os Dados dos NPCs
         "Action": "Include",
         "FromFile": "Data/NPCData/Sprites.json, Data/NPCData/Portraits.json, Data/NPCData/Dialogues.json, Data/NPCData/Schedules.json, Data/NPCData/SchedulesDialogues.json, Data/NPCData/Dispositions.json, Data/NPCData/GiftTastes.json, Data/NPCData/Animations.json, Data/NPCData/Romance.json, Data/NPCData/Mail.json, Data/NPCData/MovieData.json"
      },

      {
         //Adiciona os Dados dos Eventos
         "Action": "Include",
         "FromFile": "Data/Events/Events.json, Data/Events/EventExtras.json"
      },

      {
         //Adiciona os Dados dos Festivais
         "Action": "Include",
         "FromFile": "Data/Festivals/FestivalLocations.json, Data/Festivals/FestivalDialogues.json"
      },

      {
         //Adiciona os Dados de Alterações Dinâmicas
         "Action": "Include",
         "FromFile": "Data/DynamicChanges/MapChanges.json, Data/DynamicChanges/PortraitChanges.json, Data/DynamicChanges/SpriteChanges.json"
      },

      {
         //Adiciona Quests
         "LogName": "Include Quest Files",
         "Action":"Include",
         "FromFile": "Data/Quests/Quests.json",
      }

   ]
}